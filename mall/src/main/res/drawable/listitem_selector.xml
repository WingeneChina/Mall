<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:color="#f1f1f1" android:state_pressed="true">
        <shape>
            <stroke android:width="0.5dp" android:color="#f1f1f1"/>
        </shape>
    </item>
    <item
        android:color="#FFFFFF" android:state_pressed="false">
        <shape>
            <stroke android:width="0.5dp" android:color="#f1f1f1"/>
        </shape>
    </item>

</selector>
